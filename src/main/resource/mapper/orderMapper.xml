<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zjz.dao.OrderDao">
    <insert id="insertOrder" parameterType="com.zjz.pojo.Order">
        INSERT INTO `taotaowang`.`order` (
          `order_id`,
          `order_time`,
          `user_id`
        )
        VALUES
          (
            #{orderId},
            #{orderTime},
            #{user.userId}
          ) ;
    </insert>
</mapper>